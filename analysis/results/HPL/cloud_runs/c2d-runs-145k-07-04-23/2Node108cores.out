apiVersion: kubeflow.org/v2beta1
kind: MPIJob
metadata:
  name: hpl-mpi
spec:
  slotsPerWorker: 54
  runPolicy:
    cleanPodPolicy: Running
  mpiReplicaSpecs:
    Launcher:
      replicas: 1
      template:
        spec:
          containers:
          - image: ghcr.io/crambor/hpl:latest
            env:
              - name: OMP_NUM_THREADS
                value: "1"
              - name: OMP_PROC_BIND
                value: "TRUE"
              - name: OMP_PLACES
                value: "cores"


            name: hpl
            command:
            - mpirun
            args:
            - --allow-run-as-root
            - --bind-to-core
            - -np
            - "108"
            - ./xhpl

    Worker:
      replicas: 2
      template:
        spec:
          containers:
          - image: ghcr.io/crambor/hpl:latest
            name: hpl
            volumeMounts:
            - name: "config"
              mountPath: "/home/mpiuser/hpl/testing/HPL.dat"
              subPath: "HPL.dat"
    
            resources:
              requests:
                cpu: 54
              limits:
                cpu: 54
                memory: 200Gi

          volumes:
          - name: "config"
            configMap:
              name: "config"
